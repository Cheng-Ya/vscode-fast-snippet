{
    "name": "fast-snippet",
    "displayName": "Fast Snippet",
    "description": "",
    "version": "0.0.1",
    "publisher": "GiyyaPan",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:fastsnippet.insertText"
